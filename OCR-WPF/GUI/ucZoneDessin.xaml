<UserControl x:Class="OCR_WPF.ucZoneDessin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OCR_WPF"
             mc:Ignorable="d" 
             d:DesignHeight="128" d:DesignWidth="128">
    <Grid>
        <Canvas x:Name="pZoneDessin" HorizontalAlignment="Left" Height="128" Margin="0,0,0,0" VerticalAlignment="Top" Width="128" MouseDown="pZoneDessin_MouseDown" MouseMove="pZoneDessin_MouseMove" Background="{DynamicResource {x:Static SystemColors.ControlLightLightBrushKey}}"/>
    </Grid>
</UserControl>
